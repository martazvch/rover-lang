struct Foo {
    arr: []int,
}

fn getIdx() -> int {
    2
}

fn main() {
    var arr: []int = [1, 2, 3,]
    var arr2 = [1, 2,]
    var foo = Foo{arr=[6, 7, getIdx()]}

    // Access
    print arr[1]
    print arr[-2]

    // Element assignment
    arr[getIdx()] = 99
    // Assignment
    arr = arr2

    // Element assignment in struct
    foo.arr[getIdx()] = -9
    // Access in struct
    print foo.arr[getIdx()]
}
