struct Human {
    age: int,
    name: str,
}

fn main() {
    var human = Human.{ age = 5, name = "Tom" }
    human = Human.{ name = "Tom", age = 5 }

    var age = 95
    human = Human.{ age, name = "Tom" }

    var name = "Tom"
    human = Human.{ age = 6, name }

    human = Human.{ age, name }
    human = Human.{ name, age }
}

