enum Foo { a, b }
enum Bar {
    a,
    b,

    fn init() -> Self {
        return Self.a
    }

    fn toStr(self) -> str {
        if self == Self.a do return "a"
        return "b"
    }
}

fn main() {
    var foo = Foo.a
    var bar = Bar.b

    foo == Foo.a
    Foo.a == foo
    Foo.a == Foo.b

    bar != Bar.a
    Bar.a != bar
    Bar.a != Bar.b

    var d = Bar.init()
    print d.toStr()
}


