// All assignment should resolve to the same type and calling convention
// should be transmitted

use .data.math 

struct Foo {
    value = 8

    fn static() -> int { return 3 }

    fn method(self) -> int {
        return self.value
    }

    fn importFuncFromMethod(self) -> fn() -> int {
        return math.getInt
    }
}

fn copy() -> int {
    return 5
}

fn main() {
    var foo = Foo{}

    var bound = Foo.static
    print bound()

    bound = copy
    print bound()

    bound = foo.method
    print bound()

    bound = math.getInt
    print bound()

    // Call conv survives a layer
    var tmp = foo.importFuncFromMethod
    print tmp()
    bound = tmp()
    print bound()
}
