struct Vec2 {
    x: int,
    y: int,

    fn len(self) -> int {
        return self.x + self.y
    }
}

fn introduce() {
    print "This is a mathematical module"
}

fn getVecFromPos(x: int, y: int) -> Vec2 {
    return Vec2{ x, y }
}

