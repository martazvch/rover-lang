// All assignment should resolve to the same type and calling convention
// should be transmitted

use .data.math 

struct Foo {
    value = 8

    fn static(a = 5) -> int { return 3 }

    fn method(self, a = 5) -> int {
        return self.value
    }
}

fn copy(a = 5) -> int {
    return 5
}

fn default(c: str, a, b = 7, d, e: bool=false) { }

fn main() {
    default("abcd", a=8)
    default(e=true, a=8, c="abcd")

    var foo = Foo{}

    print Foo.static()

    print copy()

    print foo.method()

    print math.getIntDefault()
}

