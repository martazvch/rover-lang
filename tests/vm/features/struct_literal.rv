struct Human {
    age: int,
    name: str,
}

fn main() {
    var human = Human.{ age = 5, name = "Tom" }
    print human
    print human.age
    print human.name

    human = Human.{ name = "Tom", age = 5 }
    print human
    print human.age
    print human.name

    var age = 95
    human = Human.{ age, name = "Tom" }
    print human
    print human.age
    print human.name

    var name = "Martin"
    human = Human.{ age = 6, name }
    print human
    print human.age
    print human.name

    human = Human.{ age, name }
    print human
    print human.age
    print human.name
    
    human = Human.{ name, age }
    print human
    print human.age
    print human.name
}

