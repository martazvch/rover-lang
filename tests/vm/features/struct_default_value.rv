struct Vec2 {
    a, b: float,
    x: int = 8,
    y: bool,
    z, w = "yup",
    aa, bb = false,

    fn display(self) {
        print self.a
        print self.b
        print self.x
        print self.y
        print self.z
        print self.w
        print self.aa
        print self.bb
    }
}


fn main() {
    var v = Vec2 { a = 1, b = 2, y = true }
    v.display()


    v = Vec2 { y = false, x = 7, a = 3, w = "Tom",  b = 99 }
    v.display()
}
