// Multiple variables declaration

fn get_bool() -> bool { return true }
fn needs_bool(arg: bool) {}

fn main() {
    // Distribute the value
    var a, b = 1
    print a // expect: 1
    print b // expect: 1

    // Complexe expressions
    var c, d, e = 45.6, 65 * (6 - 8), get_bool()
    print c // expect: 45.6
    print d // expect: -130
    print e // expect: true

    // Infered type
    needs_bool(e) // no error

    // Null at first with any type
    var i, j, k: float
    i = 5.
    j = i * float(a) + 6.
    k = i - j * 3.
    print i // expect: 5
    print j // expect: 11
    print k // expect: -28

    // Typed + casts
    var l, m: float = 1., 56.7
    print l // expect: 1
    print m // expect: 56.7
}
