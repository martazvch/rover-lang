struct Person {
    age: int,

    fn speak(self) {
        self.age = self.age * 10
        print self.age
    }

    fn getOld(self, amount: int) {
        self.age = self.age + amount
    }
}

fn oui() {
    print "oui"
}

fn main() {
    var p = Person.{age=5}
    p.speak()

    // Mess up with stack
    var a = 5
    p.speak()
    oui()

    var method1: fn(Person) -> void 
    method1 = p.speak
    var method2: fn(Person) -> void = p.speak
    var method3 = p.speak

    method1()

    p.speak()
    method2()

    p.getOld(8)
    
    method3()
}

