enum Foo { a, b }
enum Bar {
    a,
    b,

    fn init() -> Self {
        return Self.a
    }

    fn toStr(self) -> str {
        if self == Self.a do return "a"
        return "b"
    }
}

fn main() {
    var foo = Foo.a
    print foo

    var bar = Bar.b
    print bar

    print foo == Foo.a
    print Foo.a == foo
    print Foo.a == Foo.b

    print bar != Bar.a
    print Bar.a != bar
    print Bar.a != Bar.b

    var d = Bar.init()
    print d.toStr()
}

